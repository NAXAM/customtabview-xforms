    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:d="http://xamarin.com/schemas/2014/forms/design"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:effects="clr-namespace:SignKeys.Effects;assembly=SignKeys.Effects.CustomTabView"
    xmlns:local="clr-namespace:CustomTabViewEffectDemo"
    mc:Ignorable="d"
    x:Class="CustomTabViewEffectDemo.MainPage"
    effects:TabEffect.TabBarColor="White">
    <effects:TabEffect.TabView>
        <local:TabView />
    </effects:TabEffect.TabView>
    <ContentPage
        Title="Page 1" BackgroundColor="AliceBlue">
        <!--<Grid>
            
        </Grid>-->
    </ContentPage>
    <ContentPage
        Title="Page 2" BackgroundColor="Beige"/>
    <ContentPage
        Title="Page 3" BackgroundColor="Black"/>
